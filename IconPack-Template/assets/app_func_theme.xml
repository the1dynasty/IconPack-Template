	<!-- Application Font color -->
	<!-- Application background(focus) color -->
	<AppIcon text_color="#FFFFFFFF" text_bg_color="#FF007FEB"></AppIcon>
	<!-- Theme configuration -->
	<!-- Grid Size -->
	<AppSetting grid_format="1"></AppSetting>
</Theme>

