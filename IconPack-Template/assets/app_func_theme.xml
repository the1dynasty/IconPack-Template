		bg_selected_way_of_drawing="1" bg_unselected_way_of_drawing="1" bg_color="#FFFFFFFF">
		<!-- <Bg_v way_of_drawing=""></Bg_v> <Bg_h way_of_drawing=""></Bg_h> <Bg_color></Bg_color> -->
	</ClearHistory>
