			<Focused_v way_of_drawing="1"></Focused_v> <Focused_h way_of_drawing="1"></Focused_h> 
			<CutLine_enabled></CutLine_enabled> -->
	</Tab>
	<!-- All Tab Icon -->
	<!-- Unselected Icon -->
	<!-- Selected Icon -->
	<TabIcon name="AllApps" unselected="allapp" selected="allapp_selected">
		<!-- <Unselected></Unselected> <Selected></Selected> -->
	</TabIcon>
	<!-- Recent Tab Icon -->
	<!-- Unselected Icon -->
	<!-- Selected Icon -->
	<TabIcon name="RecentApps" unselected="history" selected="history_selected">
		<!-- <Unselected></Unselected> <Selected></Selected> -->
	</TabIcon>
